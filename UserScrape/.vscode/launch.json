{
  "configurations": [
    {
      "name": "start experiment (prompt for args)",
      "args": [
        "-t",
        "${input:pickTrial}",
        "-a",
        "${input:pickAccounts}"
      ],
      "type": "python",
      "request": "launch",
      "program": "app.py",
      "console": "integratedTerminal",
    },
    {
      "name": "Python: current file",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    }
  ],
  "inputs": [
    {
      "id": "pickTrial",
      "description": "Enter a trail ID (blank for none)",
      "type": "promptString"
    },
    {
      "id": "pickAccounts",
      "description": "Enter | seperated account names (e.g. MRA, AntiSJW)",
      "type": "promptString"
    }
  ]
}